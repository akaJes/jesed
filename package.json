{
  "name": "jesed",
  "version": "0.1.0",
  "description": "online instant collaborative editor",
  "main": "index.js",
  "scripts": {
    "postinstall": "cd node_modules/ot ; npm i ; node_modules/.bin/grunt --force ; rm -r node_modules",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akaJes/jesed.git"
  },
  "config": {
    "port": 9353,
    "baseURI": "/jesed"
  },
  "configLocal": {
    "port": 3000,
    "baseURI": ""
  },
  "apps": [
    {
      "name": "jesed",
      "script": "./index.js",
      "watch": [
        "app",
        "./*.js"
      ],
      "ignore_watch": [
        "node_modules",
        "sessions"
      ],
      "watch_options": {
        "followSymlinks": false
      }
    }
  ],
  "keywords": [
    "instant",
    "collaboraion",
    "server",
    "editor"
  ],
  "author": "akaJes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/akaJes/jesed/issues"
  },
  "homepage": "https://github.com/akaJes/jesed#readme",
  "dependencies": {
    "ace-builds": "^1.3.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.1",
    "diff-match-patch": "^1.0.1",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "formidable": "^1.2.1",
    "jquery": "^3.3.1",
    "jstree": "^3.3.5",
    "mmmagic": "^0.5.0",
    "ncp": "^2.0.0",
    "ot": "git+https://github.com/Operational-Transformation/ot.js.git",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "popper.js": "^1.14.3",
    "session-file-store": "^1.2.0",
    "simple-git": "^1.92.0",
    "socket.io": "^2.1.1",
    "vex-js": "^4.1.0"
  }
}
